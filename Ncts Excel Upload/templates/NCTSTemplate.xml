<?xml version="1.0" encoding="iso-8859-1"?>
<NctsSswDeclaration xsi:noNamespaceSchemaLocation="NctsSsw.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<typeDeclaration>N</typeDeclaration>
	<version>411.1.019</version>
	<dateCreation>{dateCreation}</dateCreation>
	<timeCreation>{timeCreation}</timeCreation>

	<CustomsStreamliner>
		<template>{template}</template>
		<company>{company}</company>
		<messageStatus>{messageStatus}</messageStatus>
		<status>{status}</status>
		<LRN>{LRN}</LRN>
		<user>{user}</user>
		<printLocation>{printLocation}</printLocation>
		<createDeclaration>{createDeclaration}</createDeclaration>
		<sendingMode>O</sendingMode>
		<sendDeclaration>{sendDeclaration}</sendDeclaration>
		<sendPlannedDateTime>{sendPlannedDateTime}</sendPlannedDateTime>
		<dossierNumberERP>{commercialreference}</dossierNumberERP>
		<terminalDeclarationType>{terminalDeclarationType}</terminalDeclarationType>
		<terminal>{terminal}</terminal>
		<transhipment>{transhipment}</transhipment>

		<Principal>
			<id>{principal_id}</id>
			<ContactPerson>
				<contactPersonCode>{principal_contactPersonCode}</contactPersonCode>
			</ContactPerson>
		</Principal>

		<ControlValues>
			<ControlArticles>{ControlArticles}</ControlArticles>
			<ControlPackages>{ControlPackages}</ControlPackages>
			<ControlGrossmass>{ControlGrossmass}</ControlGrossmass>
			<ControlNetmass>{ControlNetmass}</ControlNetmass>
		</ControlValues>
	</CustomsStreamliner>

	<MessageBody>
		<GoodsDeclaration>
			<Header>
				<typeOfDeclaration>T1</typeOfDeclaration>
				<countryOfDestinationCode>{countryOfDestinationCode}</countryOfDestinationCode>
				<placeOfLoadingCode>{placeOfLoadingCode}</placeOfLoadingCode>
				<countryOfDispatchExportCode>{countryOfDispatchExportCode}</countryOfDispatchExportCode>
				<inlandTransportMode>{inlandTransportMode}</inlandTransportMode>
				<transportModeAtBorder>{transportModeAtBorder}</transportModeAtBorder>

				<identityOfMeansOfTransportAtDeparture language="EN">{identityOfMeansOfTransportAtDeparture}</identityOfMeansOfTransportAtDeparture>
				<nationalityOfMeansOfTransportAtDeparture>{nationalityOfMeansOfTransportAtDeparture}</nationalityOfMeansOfTransportAtDeparture>

				<identityOfMeansOfTransportCrossingBorder language="EN">{identityOfMeansOfTransportCrossingBorder}</identityOfMeansOfTransportCrossingBorder>
				<nationalityOfMeansOfTransportCrossingBorder>{nationalityOfMeansOfTransportCrossingBorder}</nationalityOfMeansOfTransportCrossingBorder>

				<containerisedIndicator>T</containerisedIndicator>

				<issuingDate>{issuingDate}</issuingDate>
				<dialogLanguageIndicatorAtDeparture>{dialogLanguageIndicatorAtDeparture}</dialogLanguageIndicatorAtDeparture>
				<nctsAccompanyingDocumentLanguageCode>{nctsAccompanyingDocumentLanguageCode}</nctsAccompanyingDocumentLanguageCode>

				<Totals>
					<totalNumberOfItems>{total_items}</totalNumberOfItems>
					<totalNumberOfPackages>{total_packages}</totalNumberOfPackages>
					<totalGrossMass>{total_grossmass}</totalGrossMass>
					<totalNetMass>{total_nettmass}</totalNetMass>
				</Totals>

				<simplifiedProcedureFlag>{simplifiedProcedureFlag}</simplifiedProcedureFlag>
				<declarationPlace language="EN">{declarationPlace}</declarationPlace>

				<Security>F</Security>

				<Declarant>
					<id>{declarant_id}</id>
					<name language="EN">{declarant_name}</name>
					<streetAndNumber>{declarant_streetAndNumber}</streetAndNumber>
					<postalCode>{declarant_postalCode}</postalCode>
					<City>{declarant_City}</City>
					<countryCode>{declarant_countryCode}</countryCode>
					<tin>{declarant_tin}</tin>
				</Declarant>

				<Representative>
					<user>{representative_user}</user>
					<name>{representative_name}</name>
					<authorisation language="EN">{representative_authorisation}</authorisation>
				</Representative>

				<Consignor>
					<id>{consignor_id}</id>
					<name language="NL">{consignor_name}</name>
					<streetAndNumber>{consignor_streetAndNumber}</streetAndNumber>
					<postalCode>{consignor_postalCode}</postalCode>
					<City>{consignor_City}</City>
					<countryCode>{consignor_countryCode}</countryCode>
				</Consignor>

				<Consignee>
					<id>{consignee_id}</id>
					<name language="EN">{consignee_name}</name>
					<streetAndNumber>{consignee_streetAndNumber}</streetAndNumber>
					<postalCode>{consignee_postalCode}</postalCode>
					<City>{consignee_City}</City>
					<countryCode>{consignee_countryCode}</countryCode>
				</Consignee>

				<CustomsOffices>
					<departureReferenceNumber>{departureReferenceNumber}</departureReferenceNumber>
					<destinationReferenceNumber>{destinationReferenceNumber}</destinationReferenceNumber>
				</CustomsOffices>

				<ControlResult>
					<controlResultCode>{controlResultCode}</controlResultCode>
					<dateLimit>{dateLimit}</dateLimit>
				</ControlResult>

				<Guarantee>
					<guaranteeType>{guaranteeType}</guaranteeType>
					<GuaranteeReference>
						<guaranteeReference>{guaranteeReference}</guaranteeReference>
						<accessCode>{accessCode}</accessCode>
						<notValidForEC>{notValidForEC}</notValidForEC>
					</GuaranteeReference>
				</Guarantee>

				<transportPlannedDate>{transportPlannedDate}</transportPlannedDate>
				<transportPlannedTime>{transportPlannedTime}</transportPlannedTime>
			</Header>
		</GoodsDeclaration>

		<GoodsItems>
{goods_items_xml}
		</GoodsItems>

	</MessageBody>
</NctsSswDeclaration>
