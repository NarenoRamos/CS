    <GoodsItem>
        <sequence>{sequence}</sequence> 
        <commodityCode>{commodityCode}</commodityCode> 
        <netMass>{netMass}</netMass> 
        <grossMass>{grossMass}</grossMass> 
        <goodsDescription>{goodsDescription}</goodsDescription> 
        <Packaging>
            <marksNumber>{marksNumber}</marksNumber> 
            <packages>{packages}</packages> 
            <packageType>{packageType}</packageType> 
        </Packaging>
        <destinationCountry>{destinationCountry}</destinationCountry> 
        <destinationRegion>{destinationRegion}</destinationRegion>
        <SupplementaryUnits>
            <supplementaryUnits>{supplementaryUnits}</supplementaryUnits> 
            <supplementaryUnitsCode>{supplementaryUnitsCode}</supplementaryUnitsCode> 
        </SupplementaryUnits>
        <originCountry>{originCountry}</originCountry> 
        <originRegion>{originRegion}</originRegion> 
        <CustomsTreatment>
            <Procedure>
                <procedurePart1>{procedurePart1}</procedurePart1>
                <procedurePart2>{procedurePart2}</procedurePart2>
                <procedureType>{procedureType}</procedureType>
            </Procedure>
            <Warehouse>
                <warehouseType>{warehouseType}</warehouseType>
                <warehouseIdentity>{warehouseIdentity}</warehouseIdentity>
                <warehouseCountry>{warehouseCountry}</warehouseCountry>
            </Warehouse>
            <ChargesImport>
                <VATCharges>
                    <charges>{vatCharges_charges}</charges>
                    <ExchangeRate>
                        <currency>{vatCharges_currency}</currency>
                    </ExchangeRate>
                </VATCharges>
                <CustomsCharges>
                    <transportInsuranceCharges>{customsCharges_transportInsuranceCharges}</transportInsuranceCharges>
                    <ExchangeRate>
                        <currency>{customsCharges_currency}</currency>
                    </ExchangeRate>
                </CustomsCharges>
            </ChargesImport>
        </CustomsTreatment>
        <Price>
            <price>{price}</price>
            <ExchangeRate>
                <exchangeRate>{price_exchangeRate}</exchangeRate>
                <currency>{price_currency}</currency>
            </ExchangeRate>
        </Price>
        <PreviousDocument>
            <Document>
                <documentReference>{documentReference}</documentReference> 
                <documentType>{documentType}</documentType> 
            </Document>
            <previousDocumentCategory>{previousDocumentCategory}</previousDocumentCategory> 
            <previousDocumentLoc>{previousDocumentLoc}</previousDocumentLoc> 
            <previousDocumentArt>{previousDocumentArt}</previousDocumentArt> 
            <previousDocumentDate>{previousDocumentDate}</previousDocumentDate> 
        </PreviousDocument>
    </GoodsItem>